{
  "deno.enablePaths": [
    "supabase/functions"
  ],
  "deno.lint": true,
  "deno.unstable": [
    "bare-node-builtins",
    "byonm",
    "sloppy-imports",
    "unsafe-proto",
    "webgpu",
    "broadcast-channel",
    "worker-options",
    "cron",
    "kv",
    "ffi",
    "fs",
    "http",
    "net"
  ],
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "json.schemas": [
    {
      "fileMatch": ["package.json"],
      "url": "./schemas/package-schema.json"
    },
    {
      "fileMatch": ["tsconfig.json"],
      "url": "./schemas/tsconfig-schema.json"
    }
  ],
  "http.proxySupport": "off"
}
